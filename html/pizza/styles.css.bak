html{background:#fff url(images/background.gif) repeat-x;}
body{
	width:80%; 
	margin:0 auto;
	padding:0;
	font-family:"Trebuchet MS", Helvetica, Verdana, sans-serif;
	font-size:90%;
	line-height:1.7em;
	color:#555;
	
	border: 1px solid #e00;
	}
	
	a:link{color:#006d14;}
	a:visited{color:#48a95a}
	a:hover{color:#ab0000}
	a:focus, a:active{color:#5c0000;background-color:#ddd}
	
	#branding{
			position:relative;
			height:250px;
			margin-bottom:15px;
			background:#900 url(images/background.gif) repeat-x;
		}
	
	#branding h1{margin:0; padding: 10px 0 0; color:#fff;}
	
	#address{
			color:#eee;
			position:absolute;
			top:15px;
			right:10px;
			margin:0;
			text-align:right;
			font-size:90%;
			line-height:1.3;
			border: 1px solid #fff;
		}
		
		
		#main-content{
			width:65%;
			float:right;
			border: 1px solid #f00;
			
			}
			
			#navigation{
				width:30%;
				float:left;
				margin:0;
				padding:0;
				list-style:none;
				border-top: 1px solid #b5b5b5;
				}
		
		#navigation li {
			padding: 6px 0 6px 35px;
			font:130% Georgia, "Times New Roman", Times, serif;
			border-bottom: 1px solid #b5b5b5;
			background: transparent url(images/ornament.gif) 0 50% no-repeat;
			}
		
		#navigation a{text-decoration:none;}
		
		
		
		#tagline{
			width:65%;
			position:absolute;
			top:120px;
			right:0px;
			color:fffdd7;
			margin:0;
			
			text-align:center;
			font: italic 140% Georgia, "Times New Roman", Times, serif;
			}
		
		#copyright{
			clear:both;
			text-align:right;
			font-size:80%;
			color:#888;
			padding-top:15px;
			background:transparent url(images/footerline.gif) 100% 0 no-repeat;
			
			}
			

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		